
#include "Maths.h"

namespace FleedCore
{
	//-------------------------------------------------------------------------
	FORCE_INLINE FFloat32 Maths::Abs( FFloat32 a_value )
	{
		return fabsf( a_value );
	}
	//-------------------------------------------------------------------------

	//-------------------------------------------------------------------------
	FORCE_INLINE FFloat32 Maths::Cos( FFloat32 a_angle )
	{
		return cosf( a_angle );
	}
	//-------------------------------------------------------------------------

	//-------------------------------------------------------------------------
	FORCE_INLINE FFloat32 Maths::Sin( FFloat32 a_angle )
	{
		return sinf( a_angle );
	}
	//-------------------------------------------------------------------------

	//-------------------------------------------------------------------------
	FORCE_INLINE FFloat32 Maths::Tan( FFloat32 a_angle )
	{
		return tanf( a_angle );
	}
	//-------------------------------------------------------------------------
}
